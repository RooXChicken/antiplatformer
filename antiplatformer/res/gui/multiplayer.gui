            texturebackground = "res/gui/themes/apgui.png" Part(0, 0, 31, 16) Middle(1, 1, 29, 14);
        }
    }

    Button.connect {
        Position = (20, 50);
        Size = (139, 47.1187);
        Text = "Connect to server";
        TextSize = 13;

        Renderer {
            texture = "res/gui/themes/apgui.png" Part(0, 0, 31, 16) Middle(1, 1, 29, 16);
