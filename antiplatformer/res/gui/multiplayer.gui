            texturebackground = "res/gui/themes/apgui.png" Part(0, 0, 31, 16) Middle(1, 1, 29, 14);
        }
    }

    TextBox.TextBox2 {
        DefaultText = Console;
        HorizontalScrollbarPolicy = Never;
        MaximumCharacters = 0;
